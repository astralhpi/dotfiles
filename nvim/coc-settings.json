{
  "npm.binPath": "pnpm",
  "diagnostic.errorSign": "✘",
  "diagnostic.infoSign": "",
  "diagnostic.warningSign": "⚠ ",
  "diagnostic.hintSign": "",
  "diagnostic.checkCurrentLine": true,
  "coc.source.file.shortcut": "",
  "coc.source.buffer.shortcut": "",
  "coc.source.around.shortcut": "",
  "coc.preferences.willSaveHandlerTimeout": 1000,
  "python.analysis.typeCheckingMode": "off",
  "python.formatting.provider": "black",
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "tabnine.priority": 30,
  "tabnine.shortcut":"",
  "coc.preferences.formatOnSaveFiletypes": [
    "python",
    "rust",
    "svelte",
    "dart",
    "css",
    "typescript",
    "javascript"
  ],
  "tsserver.npm": "/usr/local/bin/pnpm",
  "tsserver.formatOnType": false,
  "svelte.plugin.typescript.hover.enable": false,
  "svelte.plugin.html.hover.enable": false,
  "svelte.plugin.css.hover.enable": false,
  "suggest.autoTrigger": "trigger",
  "suggest.triggerCompletionWait": 150,
  "suggest.minTriggerInputLength": 4,
  "suggest.maxCompleteItemCount": 50,
  "suggest.floatEnable": false,
  "diagnostic-languageserver.filetypes": {
    "python": "mypy"
  },
  "diagnostic-languageserver.formatFiletypes": {
    "python": ["black", "isort"]
  },
  "diagnostic-languageserver.linters": {
    "mypy": {
      "sourceName": "mypy",
      "command": "mypy",
      "args": [
        "--no-color-output",
        "--no-error-summary",
        "--show-column-numbers",
        "--follow-imports=silent",
        "%file"
      ],
      "rootPatterns": [
        "mypy.ini",
        ".mypy.ini",
        "setup.cfg",
        "pyproject.toml"
      ],
      "formatPattern": [
        "^.*:(\\d+?):(\\d+?): ([a-z]+?): (.*)$",
        {
          "line": 1,
          "column": 2,
          "security": 3,
          "message": 4
        }
      ],
      "securities": {
        "error": "error"
      }
    } 
  },
  "svelte.enable-ts-plugin": true

}
